{
  "name": "photonic",
  "productName": "Photonic",
  "version": "0.0.1",
  "private": true,
  "description": "A photo manager for edit exif/gps/time...",
  "author": "Colin Luo <mail@luozhihua.com>",
  "scripts": {
    "clean": "node build/script.clean.js",
    "dev": "node build/script.dev.js",
    "build": "node build/script.build.js && cd electron && quasar build",
    "release": "npm run build && build -mwl",
    "release:mac": "npm run build && build -m",
    "release:win": "npm run build && build -w",
    "release:linux": "npm run build && build -l",
    "lint": "eslint --ext .js,.vue src"
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "md5": "^2.2.1",
    "moment": "^2.20.0",
    "quasar-extras": "0.x",
    "quasar-framework": "^0.14.4",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuelidate": "^0.6.1",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "colors": "^1.1.2",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.28.7",
    "electron-builder": "^19.53.7",
    "es6-promise": "^4.1.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.0",
    "json-loader": "^0.5.7",
    "opn": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss": "^6.0.13",
    "postcss-cssnext": "^3.0.2",
    "postcss-easings": "^1.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "progress-bar-webpack-plugin": "^1.10.0",
    "purify-css": "^1.2.5",
    "shelljs": "^0.8.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "build": {
    "appId": "com.colin.photonic",
    "productName": "Photonic",
    "copyright": "Copyright Â© 2018-2020 luozhihua.com",
    "directories": {
      "buildResources": "electron/icons",
      "output": "release/electron",
      "app": "dist"
    },
    "mac": {
      "category": "public.app-category.photography",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://photonic.applinks.io/"
      }
    ]
  }
}
